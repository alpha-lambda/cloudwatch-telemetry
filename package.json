{
  "name": "@alpha-lambda/cloudwatch-telemetry",
  "version": "2.0.0",
  "description": "Store custom CloudWatch metrics in a cost-effective way",
  "main": "src/DatapointsCollector.js",
  "scripts": {
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "sls deploy",
    "lint": "eslint .",
    "package": "sls package",
    "remove": "sls remove",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "env-cmd -f test/.env --no-override nyc mocha --opts test/unit/mocha.opts test/unit",
    "test:integration": "env-cmd -f test/.env --no-override mocha --opts test/integration/mocha.opts test/integration"
  },
  "keywords": [
    "aws",
    "function",
    "lambda",
    "serverless",
    "cloudwatch",
    "telemetry",
    "alpha",
    "metrics",
    "util"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alpha-lambda/cloudwatch-telemetry"
  },
  "author": {
    "name": "Anton Bazhal",
    "email": "am.bazhal@gmail.com",
    "url": "https://github.com/AntonBazhal"
  },
  "license": "MIT",
  "dependencies": {
    "@alpha-lambda/handler": "^1.3.0",
    "ajv": "^8.12.0",
    "aws-sdk": "^2.1368.0",
    "lodash.chunk": "^4.2.0",
    "pino": "^8.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.1",
    "lambda-tester": "^4.0.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "relcov": "^1.0.6",
    "serverless": "^3.30.1",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-include-dependencies": "^5.0.0",
    "serverless-prune-plugin": "^2.0.2",
    "sinon": "^11.1.2",
    "uuid": "^8.3.2"
  }
}
